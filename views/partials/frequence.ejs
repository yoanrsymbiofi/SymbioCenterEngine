<script>

    var chartFreq;
    var dataFreq;
    var optionsFreq;

    google.charts.load('current', {'packages': ['gauge']});
    google.charts.setOnLoadCallback(initChartFreq);

    function initChartFreq() {
        chartFreq = google.visualization.arrayToDataTable([
            ['Label', 'Value'],
            ['', 60]
        ]);

        optionsFreq = {
            height: 300,
            redFrom: 0, redTo: 60,
            yellowFrom: 60, yellowTo: 90,
            greenFrom: 90, greenTo: 100,
            animation: {
                duration: 1000,
                easing: 'in'
            }
        };

        chartInstantCC = new google.visualization.Gauge(document.getElementById('chartFreq'));
        chartInstantCC.draw(dataFreq, optionsFreq);

    }

    function drawChartFreq(val) {
        dataInstantCC.setValue(0, 1, val);
        chartInstantCC.draw(dataFreq, optionsFreq);
    }


</script>