<div class="cadre" style="text-align:center;border-radius:10px;padding:8px;margin:5px;height:316px;">
    <h5>Coh√©rence Cardiaque Moyenne</h5>
    <div id="chartInstantCCM" ></div>
</div>
<script>

    var chartInstantCCM;
    var dataInstantCCM;
    var optionsInstantCCM;

    google.charts.load('current', {'packages': ['gauge']});
    google.charts.setOnLoadCallback(initChartInstantCCM);

    function initChartInstantCCM() {
        dataInstantCCM = google.visualization.arrayToDataTable([
            ['Label', 'Value'],
            ['', 0]
        ]);

        optionsInstantCCM = {
            height: 300,
            redFrom: 0, redTo: 60,
            yellowFrom: 60, yellowTo: 90,
            greenFrom: 90, greenTo: 100,
            animation: {
                duration: 1000,
                easing: 'in'
            }
        };

        chartInstantCCM = new google.visualization.Gauge(document.getElementById('chartInstantCCM'));
        chartInstantCCM.draw(dataInstantCCM, optionsInstantCCM);

    }

    function drawChartCCM(val) {
        dataInstantCCM.setValue(0, 1, val);
        chartInstantCCM.draw(dataInstantCCM, optionsInstantCCM);
    }


</script>